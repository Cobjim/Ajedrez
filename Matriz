#include <iostream>
#include <vector>
using namespace std;
int main(){
	//vectores
	vector<string> vec0{"0  ","1 ","2 ","3 ","4"};
	vector<string> vec1{"a  ","R ","K ","Q ","R"};
	vector<string> vec2{"b  ","P ","P ","P ","P"};
	vector<string> vec3{"c  ",". ",". ",". ","."};
	vector<string> vec4{"d  ","p ","p ","p ","p"};
	vector<string> vec5{"e  ","r ","q ","k ","r"};
	
	// iterador
	vector<string>::iterator itr;
	
	
	//salida
	for (int i=0;i<4;i++){
		cout<<vec0[i];
	}
	cout<<vec0[4]<<endl;
	
	for (int i=0;i<4;i++){
		cout<<vec1[i];
	}
	cout<<vec1[4]<<endl;
	
	for (int i=0;i<4;i++){
		cout<<vec2[i];
	}
	cout<<vec2[4]<<endl;
	
	for (int i=0;i<4;i++){
		cout<<vec3[i];
	}
	cout<<vec3[4]<<endl;
	
	for (int i=0;i<4;i++){
		cout<<vec4[i];
	}
	cout<<vec4[4]<<endl;
	
	for (int i=0;i<4;i++){
		cout<<vec5[i];
	}
	cout<<vec5[4]<<endl;
	
}

