#include <iostream>
#include <vector>
using namespace std;
int main(){
	//vectores
	vector<string> vec0{"A","B","C","D"};
	vector<string> vec1{"T","K","Q","T"};
	vector<string> vec2{"P","P","P","P"};
	vector<string> vec3{"","","",""};
	vector<string> vec4{{"","","",""}};
	vector<string> vec5{"p","p","p","p"};
	vector<string> vec6{"t","q","k","t"};
	
	// iterador
	vector<string>::iterator itr;
	
	
	//salida
	for (int i=0;i<3;i++){
		cout<<vec0[i];
	}
	cout<<vec0[3]<<endl;
	
	for (int i=0;i<3;i++){
		cout<<vec1[i];
	}
	cout<<vec1[3]<<endl;
	
	for (int i=0;i<3;i++){
		cout<<vec2[i];
	}
	cout<<vec2[3]<<endl;
	
	for (int i=0;i<3;i++){
		cout<<vec3[i];
	}
	cout<<vec3[3]<<endl;
	
	for (int i=0;i<3;i++){
		cout<<vec4[i];
	}
	cout<<vec4[3]<<endl;
	
	for (int i=0;i<3;i++){
		cout<<vec5[i];
	}
	cout<<vec5[3]<<endl;
	
	for (int i=0;i<3;i++){
		cout<<vec6[i];
	}
	cout<<vec6[3]<<endl;
	
}
